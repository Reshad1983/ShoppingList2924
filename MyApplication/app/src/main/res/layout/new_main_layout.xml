<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:orientation="horizontal">
        <EditText
            android:id="@+id/con_search_id"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:autofillHints=""
            android:background="@drawable/hole_view"
            android:ems="10"
            android:gravity="center"
            android:hint="@string/search"
            android:inputType="text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/food_main_menu_id"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:background="@drawable/hole_view"
            android:gravity="center"
            android:text="@string/food_main"
            app:layout_constraintStart_toEndOf="@+id/con_search_id"
            app:layout_constraintTop_toTopOf="@+id/con_search_id" />

        <TextView
            android:id="@+id/con_search_btn"
            android:layout_width="80dp"
            android:layout_height="40dp"
            android:background="@drawable/hole_view"
            android:gravity="center"
            android:layout_marginStart="16dp"
            android:text="@string/search"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/food_main_menu_id"
            app:layout_constraintTop_toTopOf="@+id/con_search_id" />


    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:orientation="horizontal">

    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView

            android:id="@+id/one_con_id"
            android:layout_width="wrap_content"
            android:layout_height="57dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/hole_view"
            android:gravity="center"
            android:padding="3dp"
            android:text="1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/con_search_id" />

        <TextView
            android:id="@+id/two_con_id"
            android:layout_width="wrap_content"
            android:layout_height="53dp"
            android:layout_marginTop="11dp"
            android:background="@drawable/hole_view"
            android:gravity="center"
            android:padding="3dp"
            android:text="2"
            app:layout_constraintStart_toStartOf="@+id/one_con_id"
            app:layout_constraintTop_toBottomOf="@+id/one_con_id" />

        <TextView
            android:id="@+id/three_con_id"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/hole_view"
            android:gravity="center"
            android:padding="3dp"
            android:text="3"
            app:layout_constraintStart_toStartOf="@+id/two_con_id"
            app:layout_constraintTop_toBottomOf="@+id/two_con_id" />

        <TextView
            android:id="@+id/four_con_id"
            android:layout_width="wrap_content"
            android:layout_height="47dp"
            android:layout_marginTop="13dp"
            android:background="@drawable/hole_view"
            android:gravity="center"
            android:padding="3dp"
            android:text="4"
            app:layout_constraintStart_toStartOf="@+id/three_con_id"
            app:layout_constraintTop_toBottomOf="@+id/three_con_id" />

        <TextView
            android:id="@+id/five_con_id"
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/hole_view"
            android:gravity="center"
            android:padding="3dp"
            android:text="5"
            app:layout_constraintStart_toStartOf="@+id/four_con_id"
            app:layout_constraintTop_toBottomOf="@+id/four_con_id" />

        <TextView
            android:id="@+id/six_con_id"
            android:layout_width="wrap_content"
            android:layout_height="41dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/hole_view"
            android:gravity="center"
            android:padding="3dp"
            android:text="6"
            app:layout_constraintStart_toStartOf="@+id/five_con_id"
            app:layout_constraintTop_toBottomOf="@+id/five_con_id" />

        <TextView
            android:id="@+id/seven_con_id"
            android:layout_width="wrap_content"
            android:layout_height="38dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/hole_view"
            android:gravity="center"
            android:padding="3dp"
            android:text="7"
            app:layout_constraintStart_toStartOf="@+id/six_con_id"
            app:layout_constraintTop_toBottomOf="@+id/six_con_id" />

        <TextView
            android:id="@+id/eight_con_id"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginTop="17dp"
            android:background="@drawable/hole_view"
            android:gravity="center"
            android:padding="3dp"
            android:text="8"
            app:layout_constraintStart_toStartOf="@+id/seven_con_id"
            app:layout_constraintTop_toBottomOf="@+id/seven_con_id" />

        <TextView
            android:id="@+id/nine_con_id"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginTop="18dp"
            android:background="@drawable/hole_view"
            android:gravity="center"
            android:padding="3dp"
            android:text="9"
            app:layout_constraintStart_toStartOf="@+id/eight_con_id"
            app:layout_constraintTop_toBottomOf="@+id/eight_con_id" />
    </LinearLayout>


    <ScrollView
        android:id="@+id/con_sc_id"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="45dp"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintStart_toEndOf="@+id/nine_con_id"
        app:layout_constraintTop_toBottomOf="@+id/con_plus_id"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent=".45">

        <LinearLayout
            android:id="@+id/con_item_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:animateLayoutChanges="true"
            android:orientation="vertical"
            app:layout_constraintStart_toEndOf="@+id/six_con_id"
            app:layout_constraintTop_toTopOf="@+id/one_con_id" />
    </ScrollView>

    <ScrollView
        android:id="@+id/con_sc_id2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="45dp"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toEndOf="@+id/con_search_btn"
        app:layout_constraintStart_toEndOf="@+id/con_sc_id"
        app:layout_constraintTop_toBottomOf="@+id/con_sun_id"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent=".45">

        <LinearLayout
            android:id="@+id/con_item_view2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:animateLayoutChanges="true"
            android:orientation="vertical"
            app:layout_constraintStart_toEndOf="@+id/six_con_id2"
            app:layout_constraintTop_toTopOf="@+id/one_con_id2" />
    </ScrollView>

</LinearLayout>